{
    "tags": {
        "allowUnknownTags": false
    },
        "source": {
            "include": "dev/",
            "includePattern": ".js$",
            "excludePattern": ".+(tail|head)\\.js$"
        },
    "plugins": [
        "plugins/markdown"
    ],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "template": "node_modules/tui-jsdoc-template",
        "encoding": "utf8",
        "destination": "inline-docs/",
        "recurse": true,
        "verbose": true
    }
}